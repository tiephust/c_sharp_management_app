{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "ConnectionStrings": {
    "DefaultConnection": "Server=localhost;Database=ManagementApp;User=YOUR_USER;Password=YOUR_PASSWORD;TrustServerCertificate=true"
  },
  "JwtSettings": {
    "SecretKey": "YOUR_SECRET_KEY_HERE_MINIMUM_32_CHARACTERS",
    "Issuer": "ManagementApp",
    "Audience": "ManagementApp",
    "ExpirationInMinutes": 60
  },
  "EmailSettings": {
    "SmtpServer": "smtp.example.com",
    "SmtpPort": 587,
    "SmtpUsername": "YOUR_EMAIL",
    "SmtpPassword": "YOUR_EMAIL_PASSWORD",
    "FromEmail": "noreply@managementapp.com",
    "FromName": "Management App"
  },
  "OAuthSettings": {
    "Google": {
      "ClientId": "YOUR_GOOGLE_CLIENT_ID",
      "ClientSecret": "YOUR_GOOGLE_CLIENT_SECRET"
    },
    "Microsoft": {
      "ClientId": "YOUR_MICROSOFT_CLIENT_ID",
      "ClientSecret": "YOUR_MICROSOFT_CLIENT_SECRET"
    }
  },
  "RedisSettings": {
    "ConnectionString": "localhost:6379",
    "InstanceName": "ManagementApp:"
  }
}

